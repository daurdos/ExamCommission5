@model Phd.ViewModels.StudentGradeViewModel

@{
    ViewData["Title"] = "Протокол";
}
    <head>
        <h2 align="center">ПРОТОКОЛ № </h2>
        <br />
        <h2 align="center">Заседания Государственной аттестационной комиссии</h2>
        <h3 align="center">от <em> @DateTime.Now</em> </h3>
    </head>


    <body>
        <h4 align="center"> По рассмотрению дипломной работы (проекта) студента  </h4>
        <h3 align="center"><em> @Html.DisplayFor(model => model.StudentLName) @Html.DisplayFor(model => model.StudentFName) @Html.DisplayFor(model => model.StudentMName) @Html.DisplayFor(model => model.MajorName)</em>  </h3>
        <h4 align="left"> на тему:  </h4>
        <h3 align="center"><em>@Html.DisplayFor(model => model.StudentThesisName)</em>  </h3>
        <h4 align="left"> Присутствовали:  </h4>
        <h4 align="left"> Председатель   </h4>
        <h3 align="left"><em>@Html.DisplayFor(model => model.Chairman)</em></h3>
        <h4 align="left"> Члены: </h4>
        @foreach (var item in Model.Grades)
        {
            <h3 align="left">@Html.DisplayFor(modelItem => item.User.LastName) @Html.DisplayFor(modelItem => item.User.FirstName) @Html.DisplayFor(modelItem => item.User.MiddleName)</h3> <br />

        }
        <h4 align="left"> Дипломная работа (проект) выполнена под научным руководством: </h4>
        <h3 align="center"><em>@Html.DisplayFor(model => model.SupervisorLName) @Html.DisplayFor(model => model.SupervisorFName) @Html.DisplayFor(model => model.SupervisorMName)</em>  </h3>
        <h4 align="left"> Рецензент:  </h4>
        <h3 align="center"><em>@Html.DisplayFor(model => model.ReviewerLName) @Html.DisplayFor(model => model.ReviewerFName) @Html.DisplayFor(model => model.ReviewerMName)</em>  </h3>
        <h4 align="left"> В Государственную аттестационную комиссию представлены следующие материалы: </h4>
        <h5 align="left"> 1) текст дипломной работы (проекта)  на<em>@Html.DisplayFor(model => model.ThesPagesNumber)</em>   страницах; </h5>
        <h5 align="left"> 2) чертежи, таблицы к дипломной работе (проекту) на<em>@Html.DisplayFor(model => model.DrawTablesNumber)</em>   страницах; </h5>
        <h5 align="left"> 3) отзыв научного руководителя дипломной работы (проекта)</h5>
        <h5 align="left"> 4) рецензия на дипломную работу (проект) с оценкой. </h5>
        <h4 align="left"> Общая характеристика ответов студента на заданные ему вопросы  </h4>
        <h3 align="center"><em>@Html.DisplayFor(model => model.GeneralCharacteristic) </em>  </h3>
        <h4 align="left"> В ходе защиты дипломной работы (проекта) студент</h4>
        <h3 align="left"> <em> @Html.DisplayFor(model => model.StudentLName) @Html.DisplayFor(model => model.StudentFName) @Html.DisplayFor(model => model.StudentMName)   </em> </h3>
        <h4 align="left"> показал</h4>
        <h3 align="center"><em>@Html.DisplayFor(model => model.KnowledgeLevel) </em>  </h3>
        <h4 align="left"> Признать, что студент выполнил и защитил дипломную работу (проект) с оценкой </h4>
        <h2 align="center"><em>@Html.DisplayFor(model => model.GradeLetter) </em>  </h2>
        <h4 align="left"> Особые мнения членов комиссии: </h4>
        @foreach (var item in Model.Grades)
        {
            <h3 align="left">@Html.DisplayFor(modelItem => item.User.LastName) @Html.DisplayFor(modelItem => item.User.FirstName) @Html.DisplayFor(modelItem => item.User.MiddleName)</h3> <br />
            <h3 align="left"> @Html.DisplayFor(modelItem => item.Opinion) </h3> <br />
        }
        <h4 align="left"> Председатель государственной экзаменационной комиссии:</h4>
        <h3 align="center"><em>@Html.DisplayFor(model => model.Chairman) </em>  </h3>

        <h4 align="left"> Члены комиссии: </h4>
        @foreach (var item in Model.Grades)
        {
            <h3 align="left">@Html.DisplayFor(modelItem => item.User.LastName)</h3> <br />

        }

        <h4 align="left"> Секретарь:</h4>
        <h3 align="center"><em>@Html.DisplayFor(model => model.Secretary) </em>  </h3>
    </body>



